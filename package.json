{
    "name": "@translated-lint/zh-cn",
    "version": "0.0.32",
    "description": "简体中文提交消息检查器",
    "keywords": [
        "检查提交消息",
        "简体中文"
    ],
    "scripts": {
        "prepare": "husky",
        "build": "unbuild"
    },
    "homepage": "https://github.com/polarove/translated-lint-zh-cn",
    "repository": {
        "type": "git",
        "url": "https://github.com/polarove/translated-lint-zh-cn"
    },
    "type": "module",
    "main": "./dist/index.mjs",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.cjs"
        }
    },
    "files": [
        "dist",
        "*.mjs"
    ],
    "author": "liuqi <liuqi6602@163.com>",
    "license": "MIT",
    "devDependencies": {
        "@cz-translated-changelog/zh-cn": "latest",
        "@types/node": "^20.11.30",
        "commitizen": "^4.3.0",
        "husky": "^9.0.11"
    },
    "config": {
        "commitizen": {
            "path": "node_modules/@cz-translated-changelog/zh-cn"
        }
    },
    "publishConfig": {
        "access": "public"
    },
    "bin": {
        "tlzc": "./cli.mjs"
    },
    "dependencies": {
        "cac": "^6.7.14",
        "unbuild": "^2.0.0"
    }
}
